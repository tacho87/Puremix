          libraryTest: { error: 'Failed to load ML libraries' },
          regressionDemo: { error: 'Failed to run regression demo' },
          actionResult: actionResult
        },
        state: { mlAvailable: false, currentTab: 'libraries' }
      };
    }
  }
</loader>

